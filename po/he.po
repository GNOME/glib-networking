#: ../tls/gnutls/gtlsconnection-gnutls.c:793
#: ../tls/gnutls/gtlsconnection-gnutls.c:1374
msgid "Socket I/O timed out"
msgstr "אזל הזמן שהוקצב לקריאה/כתיבה לשקע"

#: ../tls/gnutls/gtlsconnection-gnutls.c:927
